{
  "name": "uchardet",
  "version": "1.5.0",
  "description": "Native UChardet bindings for Node",
  "keywords": [
    "uchardet",
    "libuchardet",
    "universalchardet",
    "freedesktop",
    "mozilla",
    "encoding",
    "native"
  ],
  "libuchardet": "0.0.6",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Icemanbeta/node-uchardet.git"
  },
  "author": {
    "name": "Kevin Chiu",
    "email": "chiukk1@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.10.0"
  },
  "os": [
    "win32",
    "darwin",
    "linux",
    "android"
  ],
  "dependencies": {
    "bindings": "^1.3.0",
    "node-addon-api": "^1.2.0",
    "node-gyp": "^3.6.2"
  },
  "devDependencies": {
    "ava": "^0.25.0"
  },
  "scripts": {
    "build": "node-gyp rebuild",
    "xcode": "node-gyp configure -- -f xcode",
    "test": "source .env && ava --verbose",
    "start": "source .env && node lib/index.js"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "test/**/*.js"
    ],
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "compileEnhancements": false
  }
}
